{
   "name": "m3-odin-samples",
   "version": "3.0.1",
   "license": "Apache-2.0",
   "scripts": {
      "ng": "ng",
      "start": "npm run build:libs && ng serve --proxy-config proxy.conf.json",
      "build": "ng build m3-odin-samples",
      "build-aot": "ng build m3-odin-samples --aot --prod",
      "build:lib-core": "npm run clean --prefix projects/infor-up/m3-odin && tsc -p projects/infor-up/m3-odin/tsconfig.json",
      "build:lib-angular": "ng build @infor-up/m3-odin-angular --prod",
      "build:libs": "npm run build:lib-core && npm run build:lib-angular",
      "test": "ng test m3-odin-samples --watch",
      "test:once": "ng test m3-odin-samples",
      "lint": "ng lint",
      "e2e": "ng e2e",
      "odin-documentation": "node ../cli/scripts/documentation",
      "odin-pack-documentation": "node ../cli/scripts/pack-documentation",
      "odin-pack": "node ../cli/scripts/pack",
      "odin-pack-sdk": "node ../cli/scripts/pack-sdk",
      "odin-pack-samples": "node ../cli/scripts/pack-samples",
      "odin-update-version": "node ../cli/scripts/version",
      "odin-publish": "node ../cli/scripts/publish",
      "odin-publish-version": "node ../cli/scripts/publish-version"
   },
   "private": true,
   "dependencies": {
      "@angular/animations": "~9.1.11",
      "@angular/cdk": "~9.2.3",
      "@angular/common": "~9.1.11",
      "@angular/compiler": "~9.1.11",
      "@angular/core": "~9.1.11",
      "@angular/forms": "~9.1.11",
      "@angular/material": "~9.2.3",
      "@angular/platform-browser": "~9.1.11",
      "@angular/platform-browser-dynamic": "~9.1.11",
      "@angular/router": "~9.1.11",
      "classlist.js": "^1.1.20150312",
      "highlight.js": "^9.14.2",
      "ids-enterprise-ng": "^7.2.4",
      "rxjs": "~6.5.5",
      "tslib": "^1.10.0",
      "typedoc": "^0.17.6",
      "zone.js": "~0.10.2"
   },
   "devDependencies": {
      "@angular-devkit/build-angular": "~0.901.5",
      "@angular-devkit/build-ng-packagr": "~0.901.5",
      "@angular/cli": "~9.1.5",
      "@angular/compiler-cli": "~9.1.11",
      "@types/jasmine": "~3.5.0",
      "@types/jasminewd2": "~2.0.8",
      "@types/node": "^12.11.1",
      "codelyzer": "^5.1.2",
      "jasmine-core": "~3.5.0",
      "jasmine-spec-reporter": "~4.2.1",
      "karma": "~5.0.0",
      "karma-chrome-launcher": "~3.1.0",
      "karma-coverage-istanbul-reporter": "~2.1.0",
      "karma-jasmine": "~3.0.1",
      "karma-jasmine-html-reporter": "^1.4.2",
      "ng-packagr": "^9.0.0",
      "protractor": "~5.4.4",
      "ts-node": "~8.3.0",
      "tslint": "~6.1.0",
      "typescript": "~3.8.3"
   }
}
